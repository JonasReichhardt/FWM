    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Ressources\ff123_kraftwerk.wav" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Ressources\ff123_kraftwerk.wav">
